                    <img src="@Umbraco.Media(38712).Url" alt="Network Image">
                </div>
                <div class="offer-card-text">
                    <h2 class="offer-card-text_title">@Umbraco.AssignedContentItem.Value("nfpOfferTitle_1")</h2>
                    <p class="offer-card-text_paragraph">@Umbraco.AssignedContentItem.Value("nfpOfferText_1")</p>
