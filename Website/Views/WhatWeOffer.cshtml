@inherits Umbraco.Web.Mvc.UmbracoViewPage


<section class="offers-section">
    <h2 class="offers-title">@Umbraco.AssignedContentItem.Value("nfpofferstitle")</h2>
    <div class="offers-big-container">

        <div class="offers-card-container">
            <a href="@Umbraco.Content(10842).Url">
                <div class="offer-card-picture">
                    <img src="@Umbraco.Media(38714).Url" alt="Partner deals">
                </div>
                <div class="offer-card-text">
                    <h2 class="offer-card-text_title">@Umbraco.AssignedContentItem.Value("nfpOfferTitle_3")</h2>
                    <p class="offer-card-text_paragraph">@Umbraco.AssignedContentItem.Value("nfpOfferText_3")</p>
                    <div class="offer-card-text_button"><a href="@Umbraco.Content(10842).Url">@Umbraco.GetDictionaryValue("ReadMore", "Læs mere")</a></div>
                </div>
            </a>
        </div>

        <div class="offers-card-container">
            <a href="@Umbraco.Content(10840).Url">
                <div class="offer-card-picture">
                    <img src="@Umbraco.Media(38713).Url" alt="Coaches Image">
                </div>
                <div class="offer-card-text">
                    <h2 class="offer-card-text_title">@Umbraco.AssignedContentItem.Value("nfpOfferTitle_2")</h2>
