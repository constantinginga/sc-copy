	<style>
		#main-footer > .row-container > .sc-footer {
			display: none;
		}

		#sc-newsletter {
			display: none;
		}
	</style>

	<!-- Initialize Swiper -->
	<script type="module">
		//import Swiper from 'https://unpkg.com/swiper@8/swiper-bundle.min.js'; //linkes were added in the <head> master
		//import 'https://unpkg.com/swiper@8/swiper-bundle.min.css';

		var width = (window.innerWidth > 0) ? window.innerWidth : screen.width;


		if (width <= 1000) {
			$('.gallery-thumbs p').show();
		}

		var galleryThumbs = new Swiper('.gallery-thumbs', {
			slidesPerView: 1,
			loop: true,
			freeMode: true,
			loopedSlides: 3, //looped slides should be the same
			watchSlidesProgress: true,
			speed: 1900,
		});

		var swiper = new Swiper('.swiper-h', {
			effect: 'coverflow',
			centeredSlides: true,
			slidesPerView: 'auto',
			loopedSlides: 3, //looped slides should be the same
			coverflowEffect: {
				rotate: 43,
				stretch: 130,
				depth: 1200,
				modifier: 1,
				slideShadows: false,
				freeMode: true,

			},
			loop: true,
			speed: 2000,
			autoplay: {
				delay: 4000,
			},
			thumbs: {
				swiper: galleryThumbs,
			},
			//pagination: {
			//    el: '.swiper-pagination-h',
			//    clickable: true,
			//},
		});
		var Subswiper = new Swiper('.swiper-v', {
			effect: 'fade',
			loop: true,
			speed: 1000,
			autoplay: {
				delay: 500,
			}

		});

		var Subswiper2 = new Swiper('.swiper-v2', {
			effect: 'fade',
			loop: true,
			speed: 1000,
			autoplay: {
				delay: 2500,
			}

		});
	</script>
	<!-- swiper style -->
	<style>
		.swiper {
			width: 500px;
			margin-bottom: 25px;
			border-radius: 50%;
		}

		.swiper-slide {
			background-position: center;
			background-size: cover;
			height: 300px;
		}

			.swiper-slide img {
				width: 300px;
				height: 300px;
				max-width: 100%;
				max-height: 100%;
				-ms-transform: translate(-50%, -50%);
				-webkit-transform: translate(-50%, -50%);
				-moz-transform: translate(-50%, -50%);
				transform: translate(-50%, -50%);
				position: absolute;
				left: 50%;
				top: 50%;
				border: 20px solid #47b2b2;
				border-radius: 50%;
			}
	</style>
