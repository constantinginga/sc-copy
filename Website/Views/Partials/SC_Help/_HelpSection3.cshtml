            <div class="col-12 text-center sc-heading sc-heading-black">
                <h2>@Umbraco.AssignedContentItem.Value("priceTitleMain")</h2>
            </div>
        </div>
        <div class="row text-center mt-5">
            <div class="col-md-4">
                <h3>@Umbraco.AssignedContentItem.Value("priceTitle1")</h3>
