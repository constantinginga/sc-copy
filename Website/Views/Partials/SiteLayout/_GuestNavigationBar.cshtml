<script>
    const dropdowns = document.querySelectorAll('.nfp-navbar-main-menu__item.has-dropdown');
    let submenus = document.querySelectorAll('.nfp-navbar-main-submenu');
    let submenu = document.getElementById('nfp-first-submenu');
    let toolSubmenu = document.getElementById('nfp-toolSubsection');
    const arrow = document.getElementById('nfp-arrow');

    dropdowns.forEach((d, i) => {
        d.addEventListener('mouseout', () => submenus[i].style.display = 'none');
        d.addEventListener('mouseover', () => submenus[i].style.display = 'flex');
    });
    submenus.forEach(s => {
        s.addEventListener('mouseover', () => s.style.display = 'flex');
        s.addEventListener('mouseout', () => s.style.display = 'none');
    });
  

    $("#nfp-toolSubsection_whole").hover(function () {
        if (toolSubmenu.style.display === "none") {
            toolSubmenu.style.display = "flex";
        } else {
            toolSubmenu.style.display = "none";
        }
    });

    submenu.addEventListener('mouseleave', myRemoveFunct)

    function myRemoveFunct() {
        toolSubmenu.style.display = "none";
    }


