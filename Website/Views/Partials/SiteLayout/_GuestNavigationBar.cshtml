    //        closingX.style.display = "flex";
    //        hamburger.style.display = "none";
    //        //document.body.style.overflow = "hidden";
    //    }
    //}

    function mobileNavPop() {
        var mobileNav = document.getElementById("nfp-navbar-main__mobile");
        var closingX = document.querySelector(".nfp-purple-closing");
        var hamburger = document.querySelector("#nfp-hamburger-menu");

        if (mobileNav.style.display === "block") {
            mobileNav.style.display = "none";
            closingX.style.display = "none";
            hamburger.style.display = "block";
        } else {
            mobileNav.style.display = "block";
            closingX.style.display = "block";
            hamburger.style.display = "none";
        }
    }



    let mobileSubmenu1 = document.getElementById('nfp-mobile-first-submenu');
    let mobileSubmenu2 = document.getElementById('nfp-mobile-second-submenu');
    let mobile_toolSubmenu = document.getElementById('nfp-mobile_toolSubsection'); 

    

    $(".droppingdown").click(function () {
        let svg = document.querySelector(".nfp-dropdown");

        if (mobileSubmenu1.style.display === "flex") {
            mobileSubmenu1.style.display = "none";
            //mobile_toolSubmenu.style.display = "none";
            svg.classList.remove("rotate");
        } else {
            mobileSubmenu1.style.display = "flex";
            svg.classList.add("rotate");
        }

    });

