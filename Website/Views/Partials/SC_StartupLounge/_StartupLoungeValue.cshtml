@inherits Umbraco.Web.Mvc.UmbracoViewPage
    <div class="sc-container ">
        <div class="row pt-5">
            <div class="text-center  sc-heading sc-heading-black">
                <h2>@Umbraco.AssignedContentItem.Value("valueTitle")</h2>
                <p>@Umbraco.AssignedContentItem.Value("valueSubtitle")</p>
            </div>
        </div>
        <div class="row fadeInUp animated-fast" data-animate-effect="fadeInUp">
            <div class="col-12 mx-1">
                <p>@Umbraco.AssignedContentItem.Value("valueContent")</p>
            </div>
        </div>


        <div class="row text-center">
            <div class="col-12 ">
                <a href="https://www.facebook.com/groups/startup.lounge/" class="btn btn-primary btn mb-3 mt-5">@Umbraco.AssignedContentItem.Value("buttonText")</a>
            </div>
        </div>
    </div>


