                                    <a href="https://www.avisen.dk/vi-skal-goere-det-nemmere-for-de-unge_679225.aspx">
                                        <img src="@Umbraco.Media(38318).Url" alt="Image" />
                                    </a>
                                </div>
                            </li>
                            <li class="nfp-sc-press-card splide__slide">
                                <div class="press-card__img">
                                    <a href="https://www.bizzup.dk/ivaerksaetterplatformen-startupcentral-dk-rejser-fondskapital/">
                                        <img src="@Umbraco.Media(38319).Url" alt="Image" />
                                    </a>
                                </div>
                            </li>
                            <li class="nfp-sc-press-card splide__slide">
                                <div class="press-card__img">                      
                                        <a href="https://borsen.dk/nyheder/opinion/regeringens-hjaelp-gaar-til-private-mens-erhvervslivet-maa-klare-sig-selv?b_source=startup-central&b_medium=row_1&b_campaign=list_1">
                                            <img src="@Umbraco.Media(38320).Url" alt="Image" />
                                        </a>
                                </div>
                            </li>
                            <li class="nfp-sc-press-card splide__slide">
                                <div class="press-card__img">
                                    <a href="https://erhvervplus.dk/artikel/anders-m%C3%A5tte-k%C3%A6mpe-h%C3%A5rdt-for-sin-iv%C3%A6rks%C3%A6tterdr%C3%B8m-nu-skal-et-st%C3%B8rre-millionbel%C3%B8b-hj%C3%A6lpe-med-at-g%C3%B8re-dr%C3%B8mmen-opn%C3%A5elig-for-flere">
                                            <img src="@Umbraco.Media(38321).Url" alt="Image" />
                                        </a>
                                </div>
                            </li>
                            <li class="nfp-sc-press-card splide__slide">
                                <div class="press-card__img">
                                    <a href="https://finans.dk/debat/ECE14198842/vi-skal-genfinde-danmarks-skaberkraft/">
