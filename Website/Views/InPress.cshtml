                    </div>
                </div>
            </div>
        </div>
    </section>
    

    <script src="~/scripts/splide-4.1.3/dist/js/splide.min.js"></script>
    <script>
            var pressSplide = new Splide('.nfp-sc-press-section__carousel', {
                type: 'loop',
                perPage: 8,
                perMove: 1,
                breakpoints: {
                    1450: {
                        perPage: 6
                    },
                    1250: {
                        perPage: 5
                    },
                    1000: {
                        perPage: 4
                    },
                    700: {
                        perPage: 3
                    },
                    450: {
                        perPage: 2
                    },
                    350: {
                        perPage: 1
                    }
                },
                autoplay: false,
                arrows: false,
                pagination: false,
                classes: {
                    arrows: 'splide__arrows',
                    arrow: 'splide__arrow',
                    prev: 'splide__arrow--prev arrow-left',
                    next: 'splide__arrow--next arrow-right'
                }
            });
            pressSplide.mount();
        </script>